{
    "name": "ReactNativeScrollView",
    "version": "0.0.5",
    "scripts": {
        "start": "react-native start",
        "build": "sh tools/verifyNodeVersion.sh && react-native bundle --platform ios --entry-file index.ios.js --dev false --minify true --prepack false --bundle-output ./build/fk-react-native/reactnative.ios.js",
        "buildX": "sh tools/verifyNodeVersion.sh && (wget --no-http-keep-alive -O build/fk-react-native/reactnative.ios.js \"http://localhost:8081/index.ios.bundle?platform=ios&dev=false&minify=true\") && echo DONE",
        "buildAndroid": "react-native bundle --platform android --dev false --minify true --entry-file index.android.js --bundle-output build/index.android.bundle --assets-dest android/app/src/main/res/",
        "ci": "sh ./tools/ci.sh",
        "docgen": "esdoc -c ./esdoc.json",
        "test": "sh test/setup/setup_run.sh",
        "test-console": "sh test/setup/setup_run.sh console",
        "startWeb": "webpack-dev-server --history-api-fallback --host 0.0.0.0",
        "ci-node": "sh ./deployer/ci-node.sh",
        "ci-web": "sh ./deployer/ci-web.sh",
        "web-bundle": "webpack --config webpack.config.js --progress --colors"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Flipkart/fk-react-native.git"
    },
    "keywords": [
        "react-native",
        "flipkart",
        "app",
        "react",
        "native",
        "flipkart",
        "ios",
        "flipkart",
        "android"
    ],
    "author": "flipviews",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/Flipkart/fk-react-native/issues"
    },
    "homepage": "https://github.com/Flipkart/fk-react-native",
    "dependencies": {
        "babel": "6.23.0",
        "babel-core": "^6.26.3",
        "babel-loader": "7.1.1",
        "babel-polyfill": "^6.26.0",
        "babel-preset-es2015": "6.24.1",
        "babel-preset-react": "6.24.1",
        "fk-react-native-web": "^0.1.13",
        "immutable": "3.7.6",
        "lodash": ">=4.17.5",
        "prop-types": "15.5.8",
        "raven-js": "3.5.1",
        "react": "16.4.1",
        "react-art-fiber": "0.16.2",
        "react-dom": "^16.0.0",
        "react-native": "^0.49.5",
        "react-native-htmlview": "0.12.1",
        "react-native-modalbox": "1.4.2",
        "react-native-tab-view": "0.0.74",
        "react-native-web-modalbox": "0.0.5",
        "react-native-web-tabview": "0.0.1",
        "react-navigation": "1.0.0-beta.19",
        "react-redux": "5.0.6",
        "react-router": "3.2.0",
        "react-timer-mixin": "0.13.3",
        "recyclerlistview": "1.3.4",
        "redux": "3.3.1",
        "redux-thunk": "2.0.1",
        "uuid": "3.1.0",
        "webpack": "3.3.0",
        "webpack-dev-server": "2.5.1",
        "whatwg-fetch": "^2.0.3"
    },
